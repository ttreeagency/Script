prototype(Ttree.Script:Resource) < prototype(Neos.Fusion:Tag) {
    @context.uri = ${this.uri}
    @context.defer = ${this.defer ? true : false}
    tagName = 'script'
    attributes {
        defer = ${defer ? 'defer' : null}
        src = Fusion:ResourceUri {
            path = ${uri}
        }
    }
}
